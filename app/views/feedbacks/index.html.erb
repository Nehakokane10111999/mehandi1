
<style type="text/css">
    body{
        background-image: url("https://wallpaperxyz.com/wp-content/uploads/Website-Background-Images-HD-Free-Download-Wallpaperxyz.com-11.jpg");
        
    }
    h1
        {
            text-align: center;
            font-size: 40px;
            color: #214946;
        }
    
</style>
<body class="body"><p id="notice"><%= notice %></p>

<h1 class="h1">Feedbacks</h1>

<table cellspacing="20" cellpadding="20" bgcolor="#FFFFFF" width="500" border="3" align="center">
  <thead>
    <tr>
      <th bgcolor="#FF00FF">Name</th>
      <th bgcolor="#FF00FF">Description</th>
      <th colspan="3" bgcolor="#FF00FF">Actions</th>
    </tr>
  </thead>

  <tbody>
    <% @feedbacks.each do |feedback| %>
      <tr>
        <td bgcolor="#FFCC99"><%= feedback.name %></td>
        <td bgcolor="#FFCC99"><%= feedback.description %></td>
        <td bgcolor="#FFCC99"><%= link_to 'Show', feedback %></td>
        <td bgcolor="#FFCC99"><%= link_to 'Edit', edit_feedback_path(feedback) %></td>
        <td bgcolor="#FFCC99"><%= link_to 'Destroy', feedback, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<button><%= link_to 'New Feedback', new_feedback_path %></button>
<button><%= link_to "Back ", controller: "sessions", action: 'welcome' %></button>
